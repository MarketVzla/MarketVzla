	<queryString language="SQL">
		<![CDATA[select sum(com_monto)as monto, tie_nombre,('Ingreso')as tipo
from tienda, departamento, dep_emp left join compra on com_fk_dep_emp=de_codigo
where dep_nombre='Ventas' and de_fk_departamento=dep_codigo and dep_fk_tienda=tie_codigo
group by tie_nombre,tipo
union
select sum(des_monto)as monto, tie_nombre,('Egreso')as tipo
from despacho, tienda
where tie_codigo=des_fk_tienda
group by tie_nombre, tipo
order by tie_nombre, tipo]]>
	</queryString>
