	<queryString>
		<![CDATA[select tie_nombre, des_codigo, pro_rif, pro_razonsocial, des_fechaaprobado,tel_numero, des_monto,(select distinct ((select lug_nombre from lugar where lug_tipo='parroquia' and lug_codigo=pro_fk_lugar_fiscal)||','||(select lug_nombre from lugar where lug_tipo='municipio' and lug_codigo=(select lug_fk_lugar from lugar where lug_tipo='parroquia' and lug_codigo=pro_fk_lugar_fiscal))||','||(select lug_nombre from lugar where lug_tipo='estado' and lug_codigo=(select lug_fk_lugar from lugar a where lug_tipo='municipio' and lug_codigo=(select lug_fk_lugar from lugar where lug_tipo='parroquia' and lug_codigo=pro_fk_lugar_fiscal))))as direccion
from lugar),pro_codigo,pro_nombre,(count(eje_codigo))as cantidad, max(eje_precio)as precio,(max(eje_precio)*count(eje_codigo))as total
from tienda,despacho,producto,ejemplar,proveedor left join telefono on pro_rif=tel_fk_proveedor
where tie_codigo=des_fk_tienda and des_fk_proveedor=pro_rif and des_codigo=eje_fk_despacho and eje_fk_producto=pro_codigo 
and des_codigo=$P{codigoDespacho}
group by tie_nombre,des_codigo,pro_rif,tel_numero,pro_razonsocial,des_fechaaprobado,des_monto,pro_fk_lugar_fiscal,pro_codigo,pro_nombre]]>
	</queryString>
